{
  "name": "entropia-biological-evolution-sandbox",
  "private": true,
  "version": "2.5.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:app\" \"pnpm run dev:logs\"",
    "dev:app": "vite",
    "dev:logs": "tsx scripts/log-server.ts",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "docker:up": "docker compose up -d",
    "docker:rebuild": "docker compose up -d --build",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "docker:typecheck": "docker compose exec dev pnpm run typecheck",
    "docker:test": "docker compose exec dev pnpm run test --run",
    "ne:sratb": "jscpd ./src"
  },
  "dependencies": {
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.2",
    "@react-three/postprocessing": "^3.0.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "recharts": "^3.6.0",
    "three": "^0.182.0",
    "three-stdlib": "^2.36.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.3",
    "@types/react-dom": "^19.2.3",
    "@types/three": "^0.182.0",
    "@types/ws": "^8.18.1",
    "@typescript/native-preview": "7.0.0-dev.20251226.1",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/ui": "^2.1.8",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jscpd": "^4.0.5",
    "jsdom": "^25.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "terser": "^5.44.1",
    "tsx": "^4.21.0",
    "typescript-eslint": "^8.50.1",
    "vite": "^6.2.0",
    "vite-plugin-checker": "^0.12.0",
    "vitest": "^2.1.8",
    "ws": "^8.18.3"
  }
}